<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:MOTP.ViewModel"
                    xmlns:view="clr-namespace:MOTP.View"
                    xmlns:views="clr-namespace:MOTP.View.Stations">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StationViewModel}">
        <view:StationView />
    </DataTemplate>
    
</ResourceDictionary>
